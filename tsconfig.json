{
  "compilerOptions": {
    "target": "esnext",
    "module": "CommonJS",
    "lib": [
      "esnext"
    ],
    "esModuleInterop": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "exclude": [
    "node_modules"
  ]
}